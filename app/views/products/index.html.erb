

<div class="container" id="overview">
	<p id="notice"><%= notice %></p>
	<h1>Products</h1>
  <div class="row">
  <% cache (cache_key_for_products) do %>
	    <% @products.each do |product| %>
	      	<%= render product %>
		<% end %>
	<% end %>
  </div>

<br>
<%= link_to 'New Product', new_product_path, class:"btn btn-default btn2"%>
</div>